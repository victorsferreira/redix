{
  "name": "shallow-clone",
  "description": "Make a shallow clone of an object, array or primitive.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/shallow-clone",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/shallow-clone",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "is-extendable": "^0.1.1",
    "kind-of": "^5.0.0",
    "mixin-object": "^2.0.1"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.4.2",
    "should": "^11.2.1"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assign-deep",
        "clone-deep",
        "extend-shallow",
        "is-plain-object",
        "isobject",
        "kind-of",
        "mixin-deep",
        "mixin-object"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}