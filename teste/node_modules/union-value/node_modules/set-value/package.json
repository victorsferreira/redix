{
  "name": "set-value",
  "description": "Create nested values and any intermediaries using dot notation (`'a.b.c'`) paths.",
  "version": "0.4.3",
  "homepage": "https://github.com/jonschlinkert/set-value",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/set-value",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "is-extendable": "^0.1.1",
    "is-plain-object": "^2.0.1",
    "to-object-path": "^0.3.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.2"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assign-value",
        "get-value",
        "has-value",
        "merge-value",
        "omit-value",
        "set-value",
        "union-value",
        "unset-value"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}