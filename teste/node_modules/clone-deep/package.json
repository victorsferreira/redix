{
  "name": "clone-deep",
  "description": "Recursively (deep) clone JavaScript native types, like Object, Array, RegExp, Date as well as primitives.",
  "version": "2.0.2",
  "homepage": "https://github.com/jonschlinkert/clone-deep",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/clone-deep",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "for-own": "^1.0.0",
    "is-plain-object": "^2.0.4",
    "kind-of": "^6.0.0",
    "shallow-clone": "^1.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "3.5.2"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "is-plain-object",
        "isobject",
        "kind-of",
        "shallow-clone"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}