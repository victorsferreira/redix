    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-test": "^0.2.1",
    "jest": "^24.8.0",
    "jest-file-snapshot": "^0.3.6",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "styled-components": "^4.3.1"
  },
  "dependencies": {
    "@babel/helper-annotate-as-pure": "^7.0.0",
    "@babel/helper-module-imports": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
